.titlePanel {
  border: 1px white solid;
  height: 60vh;
  padding: 0px 35px;
}

.title {
  margin-top: 30vh;
}

.title,
.title1 {
  font-size: 23px;
  font-weight: bold;
  color: #383C40;
}

.title2 {
  color: #686767;
  font-size: 12px;
  position: relative;
  height: 25px;
  line-height: 25px;
  padding-left: 7px;
  width: 200px;
}

.l_t {
  left: 0px;
  top: 0px;
}

.r_b {
  right: 0px;
  bottom: 0px;
}

.title2 image {
  position: absolute;
  height: 25px;
  width: 25px
}


.loginBtn {
  width: 86% !important;
  height: 50px;
  line-height: 33px;
  border-radius: 5px;
  background-color: #0E87EB;
  color: #fff;
  margin-top: 35px;
}

.cancelLoginBtn {
  width: 86% !important;
  border-radius: 5px;
  background-color: white;
  color: #383C40;
  margin-top: 25px;
  border: 1px #ccc solid;
  height: 50px;
  line-height: 33px;
}